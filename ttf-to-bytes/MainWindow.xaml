<Window x:Class="ttf_to_bytes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ttf_to_bytes"
        mc:Ignorable="d"
        Title="MainWindow" Height="50" Width="20">
    <Grid x:Name="grid" Background="#000000" Height="20" Width="20">
        <TextBlock x:Name="text" Text="A" FontSize="12" TextOptions.TextFormattingMode="Display" TextOptions.TextRenderingMode="Aliased" Foreground="#FFFFFF" Width="20" Height="20"/>
    </Grid>
</Window>
